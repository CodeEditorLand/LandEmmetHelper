{
	"compilerOptions": {
		"target": "es6",
		"lib": ["es2016"],
		"module": "commonjs",
		"outDir": "./lib/cjs",
		"sourceMap": true,
		"allowJs": true,
		"declaration": true,
		"esModuleInterop": true,
		"strict": true,
		"noImplicitAny": false
	},
	"exclude": ["node_modules", "lib"]
}
